{
	"name": "__MSG_extensionName__",
	"version": "3.1.5",
	"description": "__MSG_extensionDescription__",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://twitter.com/*",
				"https://api.twitter.com/*",
				"https://mobile.twitter.com/*"
			],
			"js": [
				"index.js"
			],
			"css": [
				"style.css"
			]
		}
	],
	"permissions": [
		"https://twitter.com/*",
		"https://api.twitter.com/*",
		"https://mobile.twitter.com/*",
		"storage",
		"webRequest",
		"alarms"
	],
	"manifest_version": 2,
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": true
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"default_locale": "en"
}